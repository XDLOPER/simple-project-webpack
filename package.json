{
  "name": "simple-project-webpack",
  "version": "2.0.0",
  "description": "simple webpack for simple project :)",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "tsDev": "node --loader ts-node/esm node_modules/webpack-dev-server/bin/webpack-dev-server --config ./webpack.config.dev.ts",
    "tsBuild": "node --loader ts-node/esm node_modules/webpack-cli/bin/cli.js --config ./webpack.config.prod.ts",
    "start": "webpack-dev-server --config ./dist/typescript/webpack.config.dev.js",
    "build": "webpack --config ./dist/typescript/webpack.config.prod.js"
  },
  "dependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@types/webpack": "^5.28.3",
    "@types/webpack-dev-server": "^4.7.2",
    "@types/webpack-merge": "^5.0.0",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "ejs-compiled-loader": "^3.1.0",
    "ejs-loader": "^0.5.0",
    "ejs-webpack-loader": "^2.2.2",
    "file-loader": "^6.2.0",
    "handlebars-loader": "^1.7.3",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.1",
    "jquery": "^3.7.1",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-loader": "^7.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.0",
    "sass-loader": "^13.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "url-loader": "^4.1.1",
    "webpack-dev-server": "^4.13.3",
    "webpack-merge": "^5.8.0"
  },
  "author": "y.yasir.k",
  "license": "ISC",
  "devDependencies": {
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
